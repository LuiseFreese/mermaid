{
  "name": "mermaid-to-dataverse-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fast": "vite --host localhost --port 3000 --no-open",
    "dev:turbo": "vite --host localhost --port 3000 --no-open --force",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "clear": "powershell -ExecutionPolicy Bypass -File clear-cache.ps1"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.7.1",
    "@azure/msal-react": "^2.0.11",
    "@fluentui/react-components": "^9.54.0",
    "@fluentui/react-icons": "^2.0.250",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.11.0",
    "chart.js": "^4.5.1",
    "dotenv": "^17.2.2",
    "mermaid": "^11.11.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "axe-core": "^4.10.3",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
