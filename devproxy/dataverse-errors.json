{
  "$schema": "https://raw.githubusercontent.com/dotnet/dev-proxy/main/schemas/v1.0.0/genericrandomerrorplugin.schema.json",
  "errors": [
    {
      "request": {
        "url": "https://*.dynamics.com/api/data/v9.*/EntityDefinitions*",
        "methods": ["POST"]
      },
      "responses": [
        {
          "statusCode": 503,
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json; charset=utf-8"
            },
            {
              "name": "x-ms-service-request-id",
              "value": "00000000-0000-0000-0000-000000000000"
            }
          ],
          "body": {
            "error": {
              "code": "0x80072efd",
              "message": "The server is busy and cannot process the request at this time. Please try again later."
            }
          }
        },
        {
          "statusCode": 429,
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json; charset=utf-8"
            },
            {
              "name": "Retry-After",
              "value": "60"
            },
            {
              "name": "x-ms-ratelimit-burst-remaining-xrm-requests",
              "value": "0"
            }
          ],
          "body": {
            "error": {
              "code": "0x80072322",
              "message": "Number of requests exceeded the limit of 6000 over time window of 300 seconds."
            }
          }
        },
        {
          "statusCode": 500,
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json; charset=utf-8"
            }
          ],
          "body": {
            "error": {
              "code": "0x80040217",
              "message": "An unexpected error occurred."
            }
          }
        }
      ]
    },
    {
      "request": {
        "url": "https://*.dynamics.com/api/data/v9.*/publishers*",
        "methods": ["GET", "POST"]
      },
      "responses": [
        {
          "statusCode": 503,
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json; charset=utf-8"
            }
          ],
          "body": {
            "error": {
              "code": "0x80072efd",
              "message": "The Dynamics 365 service is currently unavailable. Please try again later."
            }
          }
        },
        {
          "statusCode": 401,
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json; charset=utf-8"
            },
            {
              "name": "WWW-Authenticate",
              "value": "Bearer error=\"invalid_token\", error_description=\"The token expired\""
            }
          ],
          "body": {
            "error": {
              "code": "0x80040217",
              "message": "The authentication token has expired. Please refresh and try again."
            }
          }
        }
      ]
    },
    {
      "request": {
        "url": "https://*.dynamics.com/api/data/v9.*/solutions*",
        "methods": ["GET", "POST"]
      },
      "responses": [
        {
          "statusCode": 400,
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json; charset=utf-8"
            }
          ],
          "body": {
            "error": {
              "code": "0x80048305",
              "message": "A solution with the specified name already exists."
            }
          }
        },
        {
          "statusCode": 403,
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json; charset=utf-8"
            }
          ],
          "body": {
            "error": {
              "code": "0x80040220",
              "message": "You do not have sufficient permissions to perform this operation."
            }
          }
        }
      ]
    },
    {
      "request": {
        "url": "https://*.dynamics.com/api/data/v9.*/GlobalOptionSetDefinitions*",
        "methods": ["POST"]
      },
      "responses": [
        {
          "statusCode": 400,
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json; charset=utf-8"
            }
          ],
          "body": {
            "error": {
              "code": "0x80044246",
              "message": "A global option set with the specified name already exists."
            }
          }
        }
      ]
    },
    {
      "request": {
        "url": "https://*.dynamics.com/api/data/v9.*/RelationshipDefinitions*",
        "methods": ["POST"]
      },
      "responses": [
        {
          "statusCode": 400,
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json; charset=utf-8"
            }
          ],
          "body": {
            "error": {
              "code": "0x80048200",
              "message": "A relationship with the specified name already exists between these entities."
            }
          }
        },
        {
          "statusCode": 503,
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json; charset=utf-8"
            }
          ],
          "body": {
            "error": {
              "code": "0x80072efd",
              "message": "Service temporarily unavailable during relationship creation. Please retry."
            }
          }
        }
      ]
    }
  ]
}
